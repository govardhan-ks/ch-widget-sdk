{
  "name": "widget-sdk-example-react",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:all",
    "build:lib": "tsc -b && vite build --config vite.lib.config.ts",
    "build:iframe": "tsc -b && vite build --config vite.iframe.config.ts",
    "build:all": "npm run build:lib && npm run build:iframe",
    "serve": "node server.js",
    "start": "npm run build:all && npm run serve"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "widget-sdk-react": "file:../../packages/react",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^3.1.0",
    "typescript": "^5.3.3",
    "vite": "^4.5.0"
  }
}

