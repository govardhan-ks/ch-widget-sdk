{
  "name": "widget-sdk-example-vue",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:all",
    "build:lib": "vite build --config vite.lib.config.ts",
    "build:iframe": "vite build --config vite.iframe.config.ts",
    "build:all": "npm run build:lib && npm run build:iframe",
    "preview": "vite preview",
    "serve": "node server.js",
    "start": "npm run build:all && npm run serve"
  },
  "dependencies": {
    "vue": "^3.4.34",
    "widget-sdk-vue": "file:../../packages/vue",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.5.2",
    "typescript": "^5.3.3",
    "vite": "^4.5.0",
    "vue-tsc": "^1.8.27"
  }
}
